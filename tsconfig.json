{
    "compilerOptions": {
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "lib": ["esnext"],
      "resolveJsonModule": true,
      "module": "commonjs",
      "target": "es2017",
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
    }
  }